    "def factors(n):\n",
    "    j = 0\n",
    "    for i in range(1,n):\n",
    "        if i%3 == 0:\n",
    "            j+=i\n",
    "        elif i%5 == 0:\n",
    "            j+=i\n",
    "        else:\n",
    "            j+=0             \n",
    "    return j"
   ]
